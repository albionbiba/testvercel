<script>
    async function test () {
        const data = {
            'referenceName': 'Reference 1',
            'number': '32847293842',
            'address': 'rr x y',
            'profession': 'pa pune',
            'qualified': true, 
            'redList': true
        };
        await fetch("/marketingManager/editReference",
        {   method: "POST",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify(data)
        })
        .catch(err => console.log("Error while posting task to server", err))
        .then((response) => response.json())
            .then((rs) => console.log(rs));
    }
    </script>
    <a href="javascript:test();">Test API</a>